# sourcing
#
if [ "$(id -u)" -eq 0 ]; then
    # Running as root
    . ~/lab/dot/bashrc
else
    # Running as regular user
    . ~/usr.sh
fi

#
# desktop
#
alias theme-preset-dark='plasma-apply-lookandfeel -a org.kde.breeze.desktop && usr-ckp 1'
alias theme-preset-bright='plasma-apply-lookandfeel -a org.kde.breezedark.desktop && usr-ckp 2'
alias ypr='systemctl --user restart plasma-plasmashell'
#
