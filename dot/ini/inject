# sourcing
#
if [ "$(id -u)" -eq 0 ]; then
    # Running as root
    . ~/lab/ini/bashrc
else
    # Running as regular user
    . ~/usr.sh
fi

#
# desktop
#
alias p1='plasma-apply-lookandfeel -a org.kde.breeze.desktop && change_konsole_profile 1'
alias p2='plasma-apply-lookandfeel -a org.kde.breezedark.desktop && change_konsole_profile 2'
alias ypr='systemctl --user restart plasma-plasmashell'
